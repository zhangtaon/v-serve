{
  "name": "v-serve",
  "description": "vue devserver 基础包",
  "version": "0.1.0",
  "author": "zto",
  "main": "index.js",
  "scripts": {
    "serve": "webpack-dev-server --config ./ --open"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@v-util": "git+https://github.com/zhangtaon/v-util.git",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "element-ui": "^2.13.0",
    "eslint": "^5.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.2",
    "qs": "^6.9.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es6": true,
      "jquery": true
    },
    "globals": {
      "define": "readonly",
      "moment": "readonly"
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "rules": {
      "vue/max-attributes-per-line": "off",
      "vue/require-default-prop": "off",
      "vue/html-closing-bracket-spacing": "off",
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-self-closing": "off",
      "vue/mustache-interpolation-spacing": "never",
      "no-console": "off",
      "no-useless-escape": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "license": "MIT"
}
